{
    "env": {
      "es2021": true,  
      "node": true   
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "sourceType": "module" 
    },
    "rules": {
      "semi": "error",           
      "quotes": ["error", "single"],
      "no-console": "warn", 
      "object-curly-spacing": ["error", "always"], 
      "array-bracket-spacing": ["error", "always"]
    }
  }